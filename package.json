  "description": "CKEditor Cite Plugin ====================",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development --colors --display-error-details --config ./webpack/webpack.config.js --watch & webpack-dev-server --content-base demo/ --hot --inline --compress",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --mode production --colors --display-error-details --config ./webpack/webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
