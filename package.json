  "bugs": {
    "url": "https://github.com/joshweir/CKEditorCite/issues"
  },
  "homepage": "https://github.com/joshweir/CKEditorCite#readme",
  "dependencies": {
    "@types/ramda": "^0.25.34",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "ramda": "^0.25.0",
    "typescript": "^2.9.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "yarn": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.2",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^8.6.3",
